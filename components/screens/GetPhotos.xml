<?xml version="1.0" encoding="UTF-8"?>
<component name="GetPhotos" extends="Group">
    <children>
        <Label
            id="sessionCode"
            text=""
            horizAlign="center"
            color="#000000"
            opacity="0"
        />
        <Animation
            id="fadeInAnimation"
            repeat="false"
            easeFunction="inQuad"
            duration="1.0">
            <FloatFieldInterpolator
                id="animateOutInterp"
                key="[0.0,1.0]"
                KeyValue="[0.0,1.0]"
                fieldToInterp="sessionCode.opacity"
            />
        </Animation>
        <Animation
            id="fadeOutAnimation"
            repeat="false"
            easeFunction="inQuad"
            duration="1.0">
            <FloatFieldInterpolator
                id="animateInInterp"
                key="[0.0,1.0]"
                KeyValue="[1.0,0.0]"
                fieldToInterp="sessionCode.opacity"
            />
        </Animation>
        <GetSessionCode id="GetSessionCodeTask" />
        <PollLightSaverWebApp id="PollLightSaverWebAppTask" />
        <GetResourcePackage id="GetResourcePackageTask" />
    </children>
    <script type="text/brightscript" uri="GetPhotos.brs" />
</component>