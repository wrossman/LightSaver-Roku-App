<?xml version="1.0" encoding="utf-8"?>
<component name="GoogleWallpapers" extends="Group">
    <children>
        <Poster id="currWallpaper" loadDisplayMode="scaleToFit" />
        <Rectangle id="fadeRect" opacity="100" color="0xFFFFFF" />
        <Timer id="picTimer" repeat="true" />
        <GetGooglePhotos id="GetGooglePhotosTask" />
        <Animation
            id="fadeOutAnimation"
            repeat="false"
            easeFunction="inQuad"
            duration="1.0">
            <FloatFieldInterpolator
                id="animateOutInterp"
                key="[0.0,1.0]"
                KeyValue="[0.0,1.0]"
                fieldToInterp="fadeRect.opacity"
            />
        </Animation>
        <Animation
            id="fadeInAnimation"
            repeat="false"
            easeFunction="inQuad"
            duration="1.0">
            <FloatFieldInterpolator
                id="animateInInterp"
                key="[0.0,1.0]"
                KeyValue="[1.0,0.0]"
                fieldToInterp="fadeRect.opacity"
            />
        </Animation>
    </children>
    <script type="text/brightscript" uri="GoogleWallpapers.brs" />
</component>